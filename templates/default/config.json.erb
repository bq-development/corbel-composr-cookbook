{
    "rabbitmq.host": "<%= @rabbitmq_host %>",
    "rabbitmq.port": "<%= @rabbitmq_port %>",
    "rabbitmq.username": "<%= @rabbitmq_username %>",
    "rabbitmq.password": "<%= @rabbitmq_password %>",
    "rabbitmq.reconntimeout": <%= @rabbitmq_reconntimeout %>,
    "rabbitmq.event": "<%= @rabbitmq_event %>",
    "bootstrap.retrytimeout": <%= @bootstrap_retrytimeout %>,

    "corbel.composer.credentials": {
        "clientId": "<%= @client_id %>",
        "clientSecret": "<%= @client_secret %>",
        "scopes": "<%= @scopes %>"
    },

    "corbel.driver.options": {
        "urlBase": "<%= @urlbase %>"
    },

    "logLevel": "<%= @loglevel %>",
    "logFile": "<%= @logfile %>",
    "syslog": <%= @syslog %>
}
